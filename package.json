    "require": "./dist/create-v-model.cjs"
  },
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "preversion": "npm run build",
    "version": "npm publish --access public",
    "postversion": "git push --follow-tags",
    "test": "uvu test"
  },
  "author": "Dave Honneffer <pearofducks@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@vue/test-utils": "^2.0.0-beta.13",
    "abdomen": "^0.9.1",
    "jsdom": "^16.4.0",
    "rollup": "^2.35.1",
    "sinon": "^9.2.2",
    "uvu": "^0.5.1",
    "vue": "^3.0.4"
  }
}
